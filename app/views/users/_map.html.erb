<h2>Your MAP</h2>
〒<%= current_user.postcode %>
<%=current_user.prefecture_name%>
<%=current_user.address_city%>
<%=current_user.address_street%>
<%=current_user.address_building%>

<%=current_user.prefecture_code%>

<!-- 下の1行を追加 -->
<div id="display"></div>

<div id='map'></div>

<style>
#map {
  height: 600px;
  width: 100%;
}
</style>

<script>
let map
let geocoder
// 下の1行を追加
const display = document.getElementById('display')

function initMap(){
  geocoder = new google.maps.Geocoder()

  map = new google.maps.Map(document.getElementById('map'), {
    center: {lat: 40.7828, lng:-73.9653},
    zoom: 12,
  });

  marker = new google.maps.Marker({
    position:  {lat: 40.7828, lng:-73.9653},
    map: map
  });
}


</script>
<script src="https://maps.googleapis.com/maps/api/js?key=<%= ENV["GOOGLEMAP_API_KEY"] %>&callback=initMap" async defer></script>